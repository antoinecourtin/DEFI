<!--
    G. CONDITIONAL SECTION: EACGRP elements and attributes

    Entity name: eacgrp
    Default: IGNORE

                                                                           -->
<!--=======================================================================-->
<!ENTITY % eacgrp
  'IGNORE'
                                                                             >
<!--
    Addional conditional sections controlled by %eacgrp; will be found in
    element and attribute declaration sections below.

    The parameter entity controlling the attribute type of @id is necessary
    to ensure that when two or more eac instances are aggregated in an
    eacgrp, @id's do not conflict, and @idref's are not blind references.
                                                                           -->
<![%eacgrp;[
<!ENTITY % id.type
  'NMTOKEN'
                                                                             >
<!ENTITY % idref.type
  'NMTOKEN'
                                                                             >
]]>
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--
    PART II
    PARAMETER ENTITIES FOR ELEMENT AND ATTRIBUTE MODELS
     A. Parameter Entities for Element Models
     B. Parameter Entities for Attributes
                                                                           -->
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--
    A. Parameter Entities for Element Models
                                                                           -->
<!--=======================================================================-->
 <!ENTITY % m.ptrs
  'ptr | extptr'
                                                                             >
<!ENTITY % m.refs
  'ref | extref | bibref | title | archref | note'
                                                                             >
<!ENTITY % m.access
  'corpname | famname | persname | funact | subject | genreform'
                                                                             >
<!ENTITY % m.data
  '%m.access; | date | num | place'
                                                                             >
<!ENTITY % m.base
  'emph | lb | abbr | expan'
                                                                             >
<!ENTITY % m.base.ptrs
  '%m.base; | %m.ptrs;'
                                                                             >
<!ENTITY % m.base.ptrs.refs
  '%m.base.ptrs; | %m.refs;'
                                                                             >
<!ENTITY % m.base.ptrs.data
  '%m.base.ptrs; | %m.data;'
                                                                             >
<!ENTITY % m.base.ptrs.refs.data
  '%m.base.ptrs; | %m.data; | %m.refs;'
                                                                             >
<!ENTITY % m.inter
  'address | chronlist | list | table'
                                                                             >
<!ENTITY % m.inter.quote
  '%m.inter; | blockquote'
                                                                             >
<!ENTITY % m.blocks
  '%m.inter.quote; | p'
                                                                             >
<!-- note: m.block.noquote used in content model of blockquote
-->
<!ENTITY % m.blocks.noquote
  '%m.inter; | p'
                                                                             >
<!ENTITY % m.para.content
  '%m.base.ptrs.refs.data; | %m.inter.quote;'
                                                                             >
<!ENTITY % m.para.content.norefs
  '%m.base.ptrs.data; | %m.inter;'
                                                                             >
<!ENTITY % m.table.entry
  '%m.base.ptrs.refs.data; | address | list'
                                                                             >
<!ENTITY % m.arch
  'abstract | physdesc | descnote | repository | origination | unitdate |
   unitid | unittitle |  container | physloc | langmaterial | materialspec'
                                                                             >
<!ENTITY % m.namehead
  '(part+, ((existdate | place | nameadd)*, usedate?,
    (sourceref | sourcerefs)?, (descnote | descnotes)?))'
                                                                             >
<!ENTITY % m.bibl
  '#PCDATA | name | title | edition | imprint | bibseries | descnote'
                                                                             >
<!ENTITY % m.name
  '(#PCDATA | %m.base;)*'
                                                                             >
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--
    A. Parameter Entities for Attributes
                                                                           -->
<!--=======================================================================-->
<!ENTITY % id.type
  'ID'
                                                                             >
<!ENTITY % idref.type
  'IDREF'
>
<!ENTITY % a.common
  'id                   %id.type;                               #IMPLIED'
                                                                             >
<!ENTITY % a.ea
  'ea                   NMTOKEN                                 #IMPLIED'
                                                                             >
<!-- a.sys, a.syskey, a.type, and a.value: consitute different
     classes used in controlled type, values, rules, and
     sources
-->
<!ENTITY % a.sys
  'system               ENTITY                                  #IMPLIED
   countrycode          NMTOKEN                                 #IMPLIED
   ownercode            NMTOKEN                                 #IMPLIED'
                                                                             >
<!ENTITY % a.syskey
  'syskey               CDATA                                   #IMPLIED
   system               ENTITY                                  #IMPLIED
   countrycode          NMTOKEN                                 #IMPLIED
   ownercode            NMTOKEN                                 #IMPLIED'
                                                                             >
<!ENTITY % a.type
  'type                 CDATA                                   #IMPLIED
   typeauth             %idref.type;                            #IMPLIED
   typekey              CDATA                                   #IMPLIED'
                                                                             >
<!ENTITY % a.value
  'valueauth            %idref.type;                            #IMPLIED
   valuekey             CDATA                                   #IMPLIED'
                                                                             >
<!ENTITY % a.lang
  'languagecode         NMTOKEN                                 #IMPLIED
   scriptcode           NMTOKEN                                 #IMPLIED'
                                                                             >
<!ENTITY % a.head
  'rule                 %idref.type;                            #IMPLIED
   authorized           CDATA                                   #IMPLIED'
                                                                             >
<!ENTITY % a.date
  'scope                (begin | end | active | begin-end)      #IMPLIED
   form                 (single | closedspan | openspan)        #IMPLIED
   era                  CDATA                                   #IMPLIED
   calendar             CDATA                                   #IMPLIED
   normal               CDATA                                   #IMPLIED'
                                                                             >
<!ENTITY % av.render
  'bold | italic | bolditalic | boldunderline | boldsmcaps | boldquoted |
   nonproport | altrender | quoted | smcaps | sub | super | underline'
                                                                             >
<!-- linking attributes. this model employs xlink attributes,
     but does not employ the xlink namespace because it is
     is problematic. xslt can be used to convert instance
     using this dtd into instance with xlink namespace
     declarations and prefixing of appropriate attributes.
-->
<!ENTITY % a.simple
  'linktype             (simple)                                #FIXED
                                                                "simple"
          href          CDATA                                   #IMPLIED
          role          CDATA                                   #IMPLIED
          arcrole       CDATA                                   #IMPLIED
          title         CDATA                                   #IMPLIED
          show          (new | replace | embed | showother |
                         shownone)                              #IMPLIED
          actuate       (onload | onrequest | actuateother |
                         actuatenone)                           #IMPLIED'
                                                                             >
<!ENTITY % a.simple.in
  'targetin             %idref.type;                            #IMPLIED
   %a.simple;'
                                                                             >
<!ENTITY % a.simple.out
  'targetout            ENTITY                                  #IMPLIED
   %a.simple;'
                                                                             >
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--
    PART III
    ENCODED ARCHIVAL CONTEXT ELEMENT AND ATTRIBUTE DECLARATIONS
                                                                           -->
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::-->
<!--=======================================================================-->
<!--=======================================================================-->
<!--
    Encoded Archival Context Group
                                                                           -->
<![%eacgrp;[
<!ELEMENT eacgrp
   (eacheader, condescgrp)
                                                                             >
<!ATTLIST eacgrp
   type                 (identity | related | disparate)        #REQUIRED
   %a.common;
   %a.ea;
                                                                             >
                                                                           ]]>
<!--=======================================================================-->
<!--
    Encoded Archival Context Group
                                                                           -->
<!ELEMENT eac
   (eacheader, condesc)
                                                                             >
<!ATTLIST eac
   %a.common;
   %a.ea;
   type                 (corpname | persname | famname)       #REQUIRED
                                                                             >
<!--=======================================================================-->
<!--
    Encoded Archival Context Header
                                                                           -->
<!ELEMENT eacheader
   (eacid, mainhist, languagedecl?, ruledecl?, sourcedecl?, authdecl?)
                                                                             >
<!ATTLIST eacheader
   %a.common;
   %a.ea;
   status               (draft | edited | deleted)              #REQUIRED
   langencoding         NMTOKEN                                 'iso639-2b'
   scriptencoding       NMTOKEN                                 'iso15924'
   dateencoding         NMTOKEN                                 'iso8601'
   countryencoding      NMTOKEN                                 'iso3166-1'
   ownerencoding        NMTOKEN                                 'iso15511'
   encodinganalogsys    NMTOKEN                                 #IMPLIED
   detaillevel          (minimal | partial | full)              #IMPLIED
   %a.type;
                                                                             >
<!--=======================================================================-->
<!--
    Encoded Archival Context Identifier
                                                                           -->
<!ELEMENT eacid
   (#PCDATA)
                                                                             >
<!ATTLIST eacid
   %a.common;
   %a.ea;
   %a.syskey;
                                                                             >
<!--=======================================================================-->
